        "def sum_of_digits(n):\n",
        "  n = abs(n)\n",
        "  return sum((int(digit) for digit in str(n)))\n",
        "\n",
        "input_number = 12345\n",
        "number = sum_of_digits(input_number)\n",
        "print(number)"
